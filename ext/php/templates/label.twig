{% set child = expr.children[0] %}
{% set subres_var = '$result_' ~ child.id %}
/**
 * matching {{expr.asRhs()}}
 */
{{subres_var}} = null;
{% include expr_tpl(child) with {'expr': child} only %} 
if ({{subres_var}}) {
    $result_{{expr.id}} = new Node\Label('{{expr.label}}', $this->text, {{subres_var}}->start, {{subres_var}}->end, [{{subres_var}}]);
    $this->labels['{{expr.label}}'] = {{subres_var}};
}
