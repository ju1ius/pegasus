{% set id = varid() %}
{% set subres_var = '$result' ~ id %}
{% set child = expr.children[0] %}
/**
 * matching {{expr.asRhs()}}
 */
{{subres_var}} = null;
{% include expr_tpl(child) with {'expr': child, 'result_var': subres_var} only %} 
if ({{subres_var}}) {
    {{result_var}} = new Node\Label('{{expr.label}}', $this->text, {{subres_var}}->start, {{subres_var}}->end, [{{subres_var}}]);
}
$this->labels['{{expr.label}}'] = {{subres_var}} ?: '';
