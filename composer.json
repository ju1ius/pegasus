{
    "name": "ju1ius/pegasus",
    "description": "Parsing Expression Grammar library for PHP",
    "keywords": ["PEG", "parser", "DSL", "compiler", "parser-combinator"],
    "homepage": "https://github.com/ju1ius/pegasus",
    "type": "library",
    "license": ["GPLv2"],
    "authors": [
        {
            "name": "ju1ius",
            "homepage": "http://github.com/ju1ius",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "ju1ius\\Pegasus\\": "src/",
            "ju1ius\\Pegasus\\Compiler\\Extension\\": "extensions/"
        }
    },
    "autoload-dev": {
        "psr-4": { "ju1ius\\Pegasus\\Tests\\": "tests/Pegasus" }
    },
    "require": {
        "php": ">=5.5.0",
        "symfony/console": "^3.0",
        "twig/twig": "^1.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "symfony/var-dumper": "^3.1",
        "symfony/stopwatch": "^3.1",
        "symfony/debug": "^3.1",
        "blackfire/php-sdk": "^1.5"
    },
    "version" : "0.1",
    "bin": ["bin/pegasus"]
}
