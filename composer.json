{
  "name": "ju1ius/pegasus",
  "description": "Parsing Expression Grammar library for PHP",
  "version": "0.1",
  "homepage": "https://github.com/ju1ius/pegasus",
  "type": "library",
  "license": ["GPLv2"],
  "authors": [
    {
      "name": "ju1ius",
      "homepage": "http://github.com/ju1ius",
      "role": "Developer"
    }
  ],
  "keywords": [
    "PEG",
    "parser",
    "DSL",
    "compiler",
    "parser-combinator"
  ],
  "bin": [
    "bin/pegasus"
  ],
  "autoload": {
    "psr-4": {
      "ju1ius\\Pegasus\\": "src/",
      "ju1ius\\Pegasus\\Compiler\\Extension\\": "extensions/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ju1ius\\Pegasus\\Tests\\": "tests/Pegasus",
      "ju1ius\\Pegasus\\Examples\\": "examples/"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-mbstring": "*",
    "ext-intl": "*",
    "symfony/console": "^4.0.2",
    "twig/twig": "^2.4.4",
    "symfony/event-dispatcher": "^4.0",
    "webmozart/path-util": "^2.3"
  },
  "require-dev": {
    "symfony/var-dumper": "^4.0.2",
    "symfony/stopwatch": "^4.0.2",
    "symfony/debug": "^4.0.2",
    "blackfire/php-sdk": "^1.5"
  }
}
