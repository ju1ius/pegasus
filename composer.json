{
    "name": "ju1ius/pegasus",
    "description": "Parsing Expression Grammar library for PHP",
    "keywords": ["PEG", "parser", "DSL", "compiler", "parser-combinator"],
    "homepage": "https://github.com/ju1ius/pegasus",
    "type": "library",
    "license": ["GPLv2"],
    "authors": [
        {
            "name": "ju1ius",
            "homepage": "http://github.com/ju1ius",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "ju1ius\\Pegasus\\": "src/",
            "ju1ius\\Pegasus\\Compiler\\Extension\\": "extensions/"
        }
    },
    "autoload-dev": {
        "psr-4": { "ju1ius\\Pegasus\\Tests\\": "tests/Pegasus" }
    },
    "require": {
        "php": ">=7.1",
        "symfony/console": "^4.0.2",
        "twig/twig": "^2.4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.5",
        "symfony/var-dumper": "^4.0.2",
        "symfony/stopwatch": "^4.0.2",
        "symfony/debug": "^4.0.2",
        "blackfire/php-sdk": "^1.5"
    },
    "version" : "0.1",
    "bin": ["bin/pegasus"]
}
