{% set result_var = '$result_' ~ expr.id %}
{% set child = expr.children[0] %}
{% set subres_var = '$result_' ~ child.id %}
/**
 * Matching: {{ expr }}
 */
{{ result_var }} = null;
{{ subres_var }} = null;
{% include expr_tpl(child) with {expr: child} only %}
if (!{{ subres_var }}) {
    {{ result_var }} = new Node\Not('{{ expr.name }}', $this->text, $this->pos, $this->pos);
} else {
{{ macros.error(expr)|indent }}
}
