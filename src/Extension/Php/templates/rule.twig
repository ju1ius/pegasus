{% set result_var = result_varname(expr) %}
/**
 * Matching rule: {{ expr.name }} = {{ expr|escape_comment }}
 *
 * @return Node|null
 */
public function match_{{ expr.name }}()
{
    $scope = [];
    $this->error->rule = '{{ expr.name }}';
    {{ result_var }} = null;
    {{ render_expr(expr)|indent }}
    return {{ result_var }};
}
