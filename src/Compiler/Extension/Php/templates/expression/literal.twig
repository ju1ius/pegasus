{% set result_var = result_varname(expr) %}
{% set literal = repr(expr.literal) %}
{{ expr_comment(expr) }}
{{ result_var }} = null;
if (substr($this->text, $this->pos, {{ expr.length }}) === {{ literal }}) {
    $start = $this->pos;
    $end = $this->pos += {{ expr.length }};
    {{ result_var }} = $this->isCapturing ? new Terminal('{{ expr.name }}', $start, $end, {{ literal }}) : true;
} else {
    {{ macros.error(expr)|indent }}
}
