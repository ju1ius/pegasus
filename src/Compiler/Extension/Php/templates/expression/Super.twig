{% extends 'expr.twig' %}
{% import 'macros.twig' as h %}


{% block matching %}
{% if use_apply -%}
    {{ result_var }} = parent::apply('{{ expr.name }}', $bindings);
{%- else %}
    {{ result_var }} = parent::parse_{{ expr.name }}($bindings);
{%- endif %}
{%- endblock matching %}


{% block capturing %}
{{ block('matching') }}
{%- endblock capturing %}
