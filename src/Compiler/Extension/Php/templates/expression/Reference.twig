{% extends "expr.twig" %}
{% import "macros.twig" as h %}


{% block matching %}
{% if use_apply -%}
{{ result_var }} = $this->apply('{{ expr.identifier }}', $bindings);
{%- else %}
{{ result_var }} = $this->match_{{ expr.identifier }}($bindings);
{%- endif %}
{%- endblock matching %}


{% block capturing %}
{{ block('matching') }}
{%- endblock capturing %}
